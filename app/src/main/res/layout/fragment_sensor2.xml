<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SensorFragment2" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:fontFamily="@font/wemakeprice_bold"
                android:gravity="center"
                android:text="결정지"
                android:textSize="24sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView66"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/edge"
                    android:backgroundTint="#dbdbdb"
                    android:fontFamily="@font/wemakeprice_semibold"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:padding="4dp"
                    android:text="실내온도"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView70"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#dbdbdb"
                    android:fontFamily="@font/wemakeprice_semibold"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:padding="4dp"
                    android:text="습도"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#dbdbdb"
                    android:fontFamily="@font/wemakeprice_semibold"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:padding="4dp"
                    android:text="수위"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView73"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#dbdbdb"
                    android:fontFamily="@font/wemakeprice_semibold"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:padding="4dp"
                    android:text="수온"
                    android:textSize="18sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_indoor_temp"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#FFFFFF"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:text="00.00"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tv_indoor_temp2"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#FFFFFF"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:text="00.00"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tv_water_high"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#FFFFFF"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:text="00.00"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tv_water_temp"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#FFFFFF"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:text="00.00"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView79"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:background="@drawable/edge"
                    android:backgroundTint="#dbdbdb"
                    android:fontFamily="@font/wemakeprice_semibold"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:padding="4dp"
                    android:text="실내온도"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#dbdbdb"
                    android:fontFamily="@font/wemakeprice_semibold"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:padding="4dp"
                    android:text="습도"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView74"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#dbdbdb"
                    android:fontFamily="@font/wemakeprice_semibold"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:padding="4dp"
                    android:text="수위"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:backgroundTint="#dbdbdb"
                    android:fontFamily="@font/wemakeprice_semibold"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:padding="4dp"
                    android:text="수온"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/editTextTextPersonName3"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:ems="10"
                    android:inputType="textPersonName" />

                <EditText
                    android:id="@+id/editTextTextPersonName"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:ems="10"
                    android:inputType="textPersonName" />

                <EditText
                    android:id="@+id/editTextTextPersonName2"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:ems="10"
                    android:inputType="textPersonName" />

                <EditText
                    android:id="@+id/editTextTextPersonName4"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/edge"
                    android:ems="10"
                    android:inputType="textPersonName" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView76"
                    app:layout_constraintWidth_percent="0.5"
                    android:layout_width="240dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/edge"
                    android:backgroundTint="#dbdbdb"
                    android:fontFamily="@font/wemakeprice_semibold"
                    android:gravity="center"
                    android:outlineProvider="paddedBounds"
                    android:padding="4dp"
                    android:text="바닥열선 온도 설정"
                    android:textSize="18sp" />

                <EditText
                    android:id="@+id/editTextTextPersonName5"
                    app:layout_constraintWidth_percent="0.5"
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/edge"
                    android:ems="10"
                    android:inputType="textPersonName" />

            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>